<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appSettings>

		<add key="VirtualPathFileLink" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
		<add key="VirtualPathFileLink" value="http://localhost/Offline/Private/Pickup/ContainerDocumentation.aspx?fileGuid={0}&amp;orderGuid={1}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>

		<add key="VirtualPathFileDownload"  xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
		<add key="VirtualPathFileDownload" value="http://localhost/Offline/Private/Pickup/ContainerDocumentation.aspx?downloadFile=1&amp;fileGuid={0}&amp;orderGuid={1}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		
		<add key="VirtualPathAnonymousFileDownload" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
		<add key="VirtualPathAnonymousFileDownload" value="http://localhost/Offline/Public/ViewContainerDocumentation.aspx?downloadFile=1&amp;fileGuid={0}&amp;consignmentGuid={1}&amp;orderGuid={2}" xdt:Transform="Replace" xdt:Locator="Match(key)"/>
		
		<add key="Config.Data.Container.FileTypes"  xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
		<add key="Config.Data.Container.FileTypes" value=".csv,.pdf,.txt" xdt:Transform="Replace" xdt:Locator="Match(key)"/>

	</appSettings>
</configuration>